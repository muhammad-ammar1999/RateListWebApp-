@using ShopMs.Models

@{
    var rateList = ViewData["RateList"] as RateList;
}
<h1>Diaper Details</h1>

<div>
    <h4>RateList</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label for="dSize">Diaper Id:</label><br>
        
        </dt>
        <dd class="col-sm-10">
           @rateList.Id
        </dd>
        <dt class="col-sm-2">
            <label for="dSize">Diaper Name:</label><br>
        </dt>
        <dd class="col-sm-10">
           @rateList.DName
        </dd>
        <dt class="col-sm-2">
            <label for="dSize">Diaper Size:</label><br>
        </dt>
        <dd class="col-sm-10">
            @rateList.DSize
        </dd>
        <dt class="col-sm-2">
            <label for="dSize">Diaper Price:</label><br>
        </dt>
        <dd class="col-sm-10">
           @rateList.DPrice
        </dd>
    </dl>
</div>


<form>

    <a href="/Ratelists/Index">Back to List</a>
</form>
</div>